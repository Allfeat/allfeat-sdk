{
    "name": "@allfeat/midds-js",
    "version": "0.1.0",
    "description": "JavaScript/TypeScript SDK for Allfeat MIDDS (Music Industry Decentralized Data Structures)",
    "keywords": [
        "music",
        "midds",
        "metadata",
        "blockchain",
        "typescript",
        "sdk"
    ],
    "author": "Allfeat labs. <hello@allfeat.com>",
    "license": "GPL-3.0-only",
    "homepage": "https://allfeat.org",
    "repository": {
        "type": "git",
        "url": "https://github.com/Allfeat/allfeat-sdk",
        "directory": "packages/midds-js"
    },
    "bugs": {
        "url": "https://github.com/Allfeat/allfeat-sdk/issues"
    },
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist/**/*",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "zod": "^4.1.1"
    },
    "devDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16"
    }
}
