{
    "name": "@allfeat/midds",
    "version": "1.0.0",
    "description": "Type-only TypeScript definitions for Allfeat MIDDS (Music Industry Decentralized Data Structures)",
    "keywords": [
        "music",
        "midds",
        "metadata",
        "blockchain",
        "typescript",
        "types"
    ],
    "author": "Allfeat labs. <hello@allfeat.com>",
    "license": "GPL-3.0-only",
    "homepage": "https://allfeat.org",
    "repository": {
        "type": "git",
        "url": "https://github.com/Allfeat/allfeat-sdk",
        "directory": "packages/types/midds"
    },
    "bugs": {
        "url": "https://github.com/Allfeat/allfeat-sdk/issues"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist/**/*.d.ts",
        "README.md"
    ],
    "scripts": {
        "build": "tsc --emitDeclarationOnly",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "typescript": "^5.9.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16"
    }
}
